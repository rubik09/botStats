{
  "name": "bot-stats",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier \"src/**/*.ts\" ",
    "format:fix": "prettier --write \"src/**/*.ts\" ",
    "start": "nest start",
    "start:dev": "NODE_ENV=development nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "lint:fix": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "update-changelog": "node .gitlab/scripts/gitlab-changelog.js",
    "check:unused-exports": "npx ts-unused-exports ./tsconfig.json",
    "check:packages": "npx npm-check-updates -f '/^(?!.*eslint)/' --format group",
    "packages:fix": "ncu -u -f '/^(?!.*eslint)/' && npm i",
    "check:depcheck": "npx depcheck --ignores \"@types/multer\"",
    "dev:fix": "npm run lint:fix && npm run format:fix && npm run packages:fix",
    "pre-commit": "npm run lint && npm run format && npm run check:unused-exports && npm run check:packages && npm run check:depcheck"
  },
  "dependencies": {
    "@nestjs/axios": "^3.0.2",
    "@nestjs/common": "^10.3.10",
    "@nestjs/config": "^3.2.3",
    "@nestjs/core": "^10.3.10",
    "@nestjs/mapped-types": "*",
    "@nestjs/microservices": "10.3.10",
    "@nestjs/platform-express": "^10.3.10",
    "@nestjs/schedule": "^4.1.0",
    "@nestjs/typeorm": "^10.0.2",
    "@types/bcrypt": "^5.0.2",
    "bcrypt": "^5.1.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cron": "^3.1.7",
    "dotenv": "^16.4.5",
    "helmet": "^7.1.0",
    "kafkajs": "^2.2.4",
    "npm-check-updates": "^16.14.20",
    "pg": "^8.12.0",
    "rxjs": "^7.8.1",
    "telegram": "^2.22.2",
    "ts-unused-exports": "^10.1.0",
    "typeorm": "^0.3.20",
    "typeorm-naming-strategies": "^4.1.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.4.2",
    "@typescript-eslint/eslint-plugin": "^7.5.0",
    "@typescript-eslint/parser": "^7.5.0",
    "depcheck": "^1.4.7",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-prettier": "^5.1.3",
    "prettier": "^3.3.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.3"
  }
}
